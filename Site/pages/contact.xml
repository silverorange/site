<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatForm" id="contact_form">
		<widget class="SwatFrame" id="frame">
			<property name="title" translatable="yes">Send us a Message</property>
			<widget class="SwatFormField" id="email_to_field">
				<property name="title" translatable="yes">Send To</property>
				<widget class="SwatContentBlock" id="email_to" />
			</widget>
			<widget class="SwatFormField" id="your_email_field">
				<property name="title" translatable="yes">Your Email</property>
				<widget class="SwatEmailEntry" id="email">
					<property name="required" type="boolean">true</property>
				</widget>
			</widget>
			<widget class="SwatFormField" id="subject_field">
				<property name="title" translatable="yes">Subject</property>
				<widget class="SwatFlydown" id="subject">
					<property name="required" type="boolean">true</property>
					<property name="show_blank" type="boolean">false</property>
				</widget>
			</widget>
			<widget class="SwatFormField" id="message_field">
				<property name="title" translatable="yes">Message</property>
				<widget class="SwatTextarea" id="message">
					<property name="required" type="boolean">true</property>
				</widget>
			</widget>
			<widget class="SwatFooterFormField">
				<widget class="SwatButton">
					<property name="title" translatable="yes">Send Message</property>
				</widget>
			</widget>
		</widget>
	</widget>
</swatml>
